@model List<LawnPaynes.Models.Customer>

@{
    ViewBag.Title = "Customers";
}

<h2>@ViewBag.Title</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th> Customer Name</th>
            <th> New Customer?</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var customer in Model)
        {
            <tr>
                <td>@customer.Name</td>

            </tr>
        }
    </tbody>
</table>

